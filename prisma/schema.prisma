

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  userId      String   @id @default(cuid())
  firstName   String?     
  lastName    String?   
  email       String   @unique
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  clerkUserId String?  @unique
}
